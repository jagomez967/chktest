@using Reporting.ViewModels
@model FiltrosViewModel
@{
    ViewBag.Title = Reporting.Resources.Mapas.titulo;
    string currentDate = @DateTime.Today.ToString("yyyy-MM-dd");
}

@section styles{
    <style>
        body {
            padding-top: 45px;
            padding-left: 40px;
            margin: 0;
        }

        html, body, #map {
            height: 100%;
            font: 10pt "Helvetica Neue", Arial, Helvetica, sans-serif;
        }

        #renderBody > .modal-backdrop {
            z-index: 9000;
        }

        .header-description {
            margin-top: 10px;
            padding: 16px 24px 20px;
            color: white;
            background-color: #4285F4;
            margin-right: 40px;
        }

        .section-header-title {
            font-size: 18px;
            overflow: hidden;
            text-overflow: ellipsis
        }

        .section-header-subtitle {
            font-size: 13px;
            margin-top: 4px
        }

        .section-header-subtitle-badge {
            display: inline-block;
            font-weight: bold;
            margin: 2px 0;
            padding: 2px 0px;
        }

        .section-divider-bottom-line {
            border-bottom: 1px solid #e6e6e6;
        }

        .section-divider {
            -webkit-flex: none;
            -ms-flex: none;
            flex: none;
        }

        .section-info {
            font-size: 13px;
            -webkit-flex: none;
            -ms-flex: none;
            flex: none;
            position: relative;
        }

        .section-info-action-button {
            display: inherit;
        }

        .section-info-line {
            padding: 5px 24px;
            display: -ms-flexbox;
            display: -webkit-flex;
            display: flex;
            -webkit-align-items: center;
            -ms-flex-align: center;
            align-items: center;
            position: relative;
            font-size: 13.5px;
        }

        .section-info-icon {
            margin-right: 10px;
            -webkit-flex: 0 0 24px;
            -ms-flex: 0 0 24px;
            flex: 0 0 24px;
            color: #003e8b;
        }

        .section-info-text, .section-info-text .widget-pane-link {
            color: rgba(0, 0, 0, 0.87);
            text-align: left;
            font-family: 'Avenir Next Medium';
        }

        .section-info-hoverable:hover {
            background-color: rgba(0,0,0,.03);
            cursor: pointer;
        }

        .section-last-report {
            padding-right: 40px;
        }

        .section-photo-reports {
            display: -webkit-box;
            margin-right: 40px;
            height: auto;
            white-space: nowrap;
            overflow: scroll;
            -webkit-flex: none;
            -ms-flex: none;
            flex: none;
            transform: translateZ(0);
        }

        .section-image-button {
            cursor: pointer;
            position: relative;
            display: inline-block;
        }

        .section-image-container {
            height: 106px;
            position: relative;
            overflow: hidden;
            z-index: -1;
            direction: ltr;
            margin: 3px;
            z-index: 9999999;
        }

            .section-image-container > a > img {
                position: relative;
                top: -1px;
                left: 0px;
                width: 130px;
                height: 107px;
                display: block;
            }

        .gm-style > div > div > div > div > img {
            max-width: none;
            border-radius: 25px !important;
            border-style: ridge !important;
            border-color: darkblue !important;
            border-width: 2px !important;
        }

        .section-row-report {
            font-size: 13px;
            padding: 0 0 16px;
            -webkit-flex: none;
            -ms-flex: none;
            flex: none;
            position: relative;
            color: rgba(0, 0, 0, 0.87);
        }

        .section-row-report-icon {
            margin-top: 6px;
            height: 40px;
            width: 40px;
            border-radius: 20px;
            border-width: 2px;
            border-style: solid;
            border-color: black;
        }

        .section-row-report-content {
            position: absolute;
        }

        .section-row-line {
            padding-left: 55px;
            line-height: 19px;
            min-height: 40px;
        }

        .section-row-line-text {
            font-family: 'Avenir Next Medium';
            font-size: 14px;
        }

        .photoListPDV {
            display: -webkit-box;
        }

        ._trackingUserList {
            padding-top: 15px;
            padding-right: 20px;
        }

        ._trackingLoader {
            left: 250px;
            display: block;
            position: fixed;
            top: 107px;
        }

        .DateUserTracking {
            margin: 10px;
        }

        #DailyTrackingUserList > ul {
            list-style-type: none;
        }

            #DailyTrackingUserList > ul > li {
                display: flex;
                margin: 4px;
                margin-left: -35px;
            }

        .selected {
            font-weight: bold;
            color: darkorange;
        }

        .nameUserProfileTracking {
            padding: 10px;
        }

        .GeoProgressbar {
            counter-reset: step;
            padding: 0px;
        }

            .GeoProgressbar li {
                list-style-type: none;
                float: left;
                width: 22%;
                position: relative;
                text-align: center;
            }

                .GeoProgressbar li:before {
                    content: counter(step);
                    counter-increment: step;
                    width: 25px;
                    height: 25px;
                    line-height: 20px;
                    border: 1px solid #ddd;
                    display: block;
                    text-align: center;
                    margin: 0 auto 10px auto;
                    border-radius: 50%;
                    background-color: white;
                    font-weight: bold;
                }

                .GeoProgressbar li:after {
                    content: '';
                    position: absolute;
                    width: 100%;
                    height: 5px;
                    background-color: #ddd;
                    top: 10px;
                    left: -50%;
                    z-index: -1;
                }

                .GeoProgressbar li:first-child:after {
                    content: none;
                }

                .GeoProgressbar li.active { /*el color del contenido del elemento li*/
                    color: black;
                    font-weight: bold;
                }

                .GeoProgressbar li.dateError {
                    color: white;
                    font-weight: bold;
                }

                .GeoProgressbar li.active:before { /*el borde de la linea del activo*/
                    border-color: #0245F4;
                    border-width: 3px;
                    background-color: #4285F4;
                }

                .GeoProgressbar li.dateError:before {
                    border-color: darkred;
                    border-width: 2px;
                    background-color: red;
                }

                .GeoProgressbar li.active + li:after {
                    background-color: #4285F4;
                }

                .GeoProgressbar li.dateError + li:after {
                    background-color: darkred;
                }
    </style>
}

<div id="sidebar" class="sidebar collapsed">
    <div class="sidebar-tabs">
        <ul role="tablist">
            <li><a id="selectGeneralMap" href="#filterMap" role="tab"><i class="fa fa-bars"></i></a></li>
            <li><a id="selectUserMap" href="#userMap" role="tab"><i class="fa fa-user"></i></a></li>
        </ul>
    </div>

    <div class="sidebar-content" style="z-index: 0;">
        <div class="sidebar-pane" id="filterMap">
            <h1 class="sidebar-header">@Reporting.Resources.Mapas.lblFilter <span class="sidebar-close"><i class="fa fa-caret-left"></i></span></h1>

            @using (Ajax.BeginForm("AplicarFiltros", "Geo", null, new AjaxOptions { OnBegin = "aplicarfiltrobegin", OnSuccess = "aplicarfiltrosuccess", OnComplete = "aplicarfiltrocomplete", InsertionMode = InsertionMode.Replace, UpdateTargetId = "filtrostablero" }))
            {
                @Html.AntiForgeryToken()
                <div>
                    <div class="filtroacciones" style="right: 10px;position: absolute;z-index: 99999;">
                        <button type="submit" class="btn aplicarfiltro">
                            @Reporting.Resources.Mapas.btnMapasAplicarFiltroLabel
                            <i class="fa fa-check pull-right"></i>
                        </button>
                    </div>
                    <div id="filtrostablero" class="filtroitems"></div>
                </div>
            }
        </div>

        <div class="sidebar-pane" id="userMap">
            <h1 class="sidebar-header">
                @Reporting.Resources.Mapas.lblUserTracking
                <span class="sidebar-close"><i class="fa fa-caret-left"></i></span>
            </h1>

            <div id="TrackingUserLoad" class="_trackingLoader"></div>
            <div id="TrackingUserList" class="_trackingUserList"></div>
        </div>

        <div class="sidebar-pane" id="infoMap">
            <h1 class="sidebar-header">
                @Reporting.Resources.Mapas.lblDetails
                <span class="sidebar-close"><i class="fa fa-caret-left"></i></span>
            </h1>
            <div id="infoMapDetail" style="margin:-10px -20px 0;"></div>
        </div>
    </div>
</div>

<div class="modal fade foto-ampliada" id="modalAmpliarImagen" tabindex="-1" role="dialog" aria-labelledby="tituloModalAmpliar" style="z-index:9001">
    <div class="modal-dialog" role="document" style="background:mintcream;width:50%">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        </div>
        <div class="modal-body">
            <div class="modal-content"></div>
        </div>

    </div>
</div>

<div id="menuDatosCapa" style="height: 50px;">
    <div id="tiemporeal">
        <input class="form-control input-sm" type="text" id="seachUserRealTime" />
        <div id="tiemporealUsuarios">
        </div>
    </div>
</div>
<div id="map" class="sidebar-map" style="margin-top:0px;bottom: 50px;"></div>

<input id="HD_DailyUserTrackingDate" type="hidden" value="@currentDate" />

@section scripts{
    @Scripts.Render("~/bundles/unobstrusive")
    <script src="~/Scripts/jquery-sidebar.js"></script>
    <script src="~/Scripts/firebase.js"></script>
    <script src="~/Scripts/firebase-app.js"></script>
    <script src="~/Scripts/firebase-auth.js"></script>
    <script src="~/Scripts/firebase-database.js"></script>
    <script async defer src="~/Scripts/map-icons.js"></script>
    <script async defer src="~/Scripts/markerclusterer.js"></script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCYyiu28Rs1AeAvAFLp60uUkRWcw3qrgK0&callback=initMap"></script>
    <script async defer src="~/Scripts/oms.min.js?spiderfier_callback=initMap"></script>

    <script>
        $(document).ready(() => {
            GetFiltrosDeTablero();
            let nowDate =$('#HD_DailyUserTrackingDate').val().replace(/-/g, "");
            getTrackingUserList(nowDate);
        });

        const sidebar = $('#sidebar').sidebar();
        const markersRealtime = [], markersPDV = [], markersUserTracking = [], markersUserGeo = [];

        let mapLibsReady = 0;
        let mc = null; //MARKER CLUSTER
        let oms = null;

        initMap = () => {
            if (++mapLibsReady < 2) return;

            const mapProp = {
                center: new google.maps.LatLng(-34.603722, -58.381592),
                zoom: 10,
                mapTypeId: google.maps.MapTypeId.ROADMAP
            };

            map = new google.maps.Map(document.getElementById('map'), mapProp);

            oms = new OverlappingMarkerSpiderfier(map, {
                markersWontMove: true,
                markersWontHide: true,
                basicFormatEvents: true
            });

            getMarkersCobertura();
            showTiempoReal();
        }

        const GetFiltrosDeTablero = () =>
            $.ajax({
                url: "@Url.Action("GetFiltrosPartialSimpleLayout", "Geo")",
                type: "POST",
                dataType: "html",
                beforeSend: ()  => $('.filtroitems').empty().html("<h3>@Reporting.Resources.Mapas.loadingFilter</h3>"),
                success: data => $(".filtroitems").html(data),
                complete: () => {if (!Modernizr.inputtypes.date) setFechasDowngradeBrowsers(); }
            });

         aplicarfiltrosuccess = () => {
            removeMarkers();
             getMarkersCobertura();
             showTiempoReal();
        }

         aplicarfiltrobegin = () => $('.aplicarfiltro').prop("disabled", true);

         aplicarfiltrocomplete = () => {
            $('.aplicarfiltro').prop("disabled", false);
            GetFiltrosDeTablero();
        }

        const removeMarkers = () => {
            markersPDV.forEach(item => item.setMap(null));
            markersPDV.splice(0);

            markersRealtime.forEach(item => item.setMap(null));
            markersRealtime.splice(0);
        }

        const GetUsuariosCliente = () => {
            return $.ajax({
                url: "@Url.Action("GetUsuariosCliente", "Geo")",
                contentType: "application/json; charset=utf-8",
                type: 'POST',
                async: false,
                dataType: 'json',
                success: data => { if (data.length == 0) data }
            }).responseJSON;
        };

        const GetClienteDiferenciaHoraria = () => {
            return $.ajax({
                url: "@Url.Action("GetClienteDifHora", "Geo")",
                contentType: "application/json; charset=utf-8",
                type: 'POST',
                async: false,
                dataType: 'json',
                success: data => { if (data.length == 0) data }
            }).responseJSON;
        };

        const showRealTimeUser = (idUser) => {
            const activeMarker = markersRealtime.find(m => m.idUser != idUser && m.visible == true);
            const userMarker = markersRealtime.find(m => m.idUser == idUser);
            if (activeMarker) activeMarker.setVisible(false);
            if (userMarker) userMarker.setVisible(true);
        };

        const showTiempoReal = () => {
            if (!firebase.apps.length) {
                firebase.initializeApp(_CONFIG);
                const auth = firebase.auth();
                auth.signInWithEmailAndPassword("checkposdev@checkpos.com", "Test123")
                    .catch(error => console.log("Err: " + error.code + " " + error.message));
            }

            const UserList = GetUsuariosCliente().map(u => ({
                id: u.idUsuario,
                name: u.nombreApellido,
                img: u.imagen
            }));

            const clientDifHH = GetClienteDiferenciaHoraria();  

            UserList.forEach(user => {
                const db = firebase.database().ref("Pos/" + user.id);
                let i = null;

                db.on("value", snap => {
                    if (snap.val()) {
                        let { timestamp, lat, long } = snap.val();
                        //Hora 3600 Segundos * 1000
                        timestamp += clientDifHH * 3600 * 1000;
                        const date = new Date(timestamp.toString().substring(0, 10) * 1000);
                        const finaldate = [('00' + date.getDate()).slice(-2), ('00' + (date.getMonth() + 1)).slice(-2),
                        date.getFullYear()].join('/') + ' ' +
                            [date.getHours(),
                            ('00' + date.getMinutes()).slice(-2),
                            ('00' + date.getSeconds()).slice(-2)].join(':');

                        const pos = { lat: parseFloat(lat), lng: parseFloat(long) }
                        let UserImage    = { scaledSize: new google.maps.Size(35, 35)};
                        let UserImageBig = { scaledSize: new google.maps.Size(50, 50)};

                        if (user.img) {
                            UserImage.url = 'data:image/png;base64,' + user.img;
                        } else {
                            UserImage.url = _USER_DEFAULT_IMG;
                        }

                        UserImageBig.url = UserImage.url;

                        if (!user.m) {
                            user.m = new google.maps.Marker({
                                position: pos,
                                map: map,
                                idUser: user.id,
                                icon: UserImage,
                                optimized: false,
                                originalIcon: UserImage,
                                hoverIcon: UserImageBig
                            });
                            markersRealtime.push(user.m);

                            i = new google.maps.InfoWindow({
                                content: `<h1>${user.name}</h1></br>
                                    <p><b>Latitud: </b><span id="map_${user.id}_lat">${lat}</span></br>
                                    <b>Longitud:  </b><span id="map_${user.id}_long">${long}</span></br>
                                    <b>Fecha:     </b><span id="map_${user.id}_fecha">${finaldate}</span></p>`,
                                position: pos
                            });
                            i.close()

                            user.m.addListener('click', () => i.open(map, user.m));
                            user.m.addListener('mouseover', function () { this.setIcon(this.hoverIcon); });
                            user.m.addListener('mouseout', function () { this.setIcon(this.originalIcon); });
                        }
                        else {
                            user.m.setPosition(pos);
                            user.m.setAnimation(google.maps.Animation.BOUNCE);
                            setTimeout(function () { user.m.setAnimation(null); }, 1400);
                            $('#map_' + user.id + '_lat').text(lat);
                            $('#map_' + user.id + '_long').text(long);
                            $('#map_' + user.id + '_fecha').text(finaldate);
                        }
                    }
                });

            });
        };

        const getMarkersCobertura = () => (
        $.ajax({
            url: "@Url.Action("GetPDVsCliente","Geo")",
            contentType: "application/json; charset=utf-8",
            type: 'POST',
            dataType: 'json',
            success: (data) => {
                if (data && data.Markers && data.Markers.length > 0) {
                    let mapbounds = new google.maps.LatLngBounds();

                    $.each(data.Markers, (key,{ position, visitado, idPuntoDeVenta, icon, ultimoReporte, usuario }) => {
                        let marker = new google.maps.Marker({
                            position: position,
                            map: map,
                            visitado: visitado,
                            idPuntoDeVenta: idPuntoDeVenta,
                            icon: { scaledSize: new google.maps.Size(30, 30),url: "@Url.Content("~/images/")" + icon},
                            ultimoReporte: ultimoReporte,
                            usuario: usuario,
                            optimized: true
                        });
                        mapbounds.extend(marker.position);
                        markersPDV.push(marker);
                        oms.addMarker(marker);
                        google.maps.event.addListener(marker, 'spider_click', () => viewPlacesDetails(idPuntoDeVenta, usuario, ultimoReporte));
                    });

                    map.setCenter(mapbounds.getCenter());
                    map.fitBounds(mapbounds);

                    if(mc != null) mc.clearMarkers();
                    mc = new MarkerClusterer(map, markersPDV, mcOptions);
                }
            }
            })
        )

        const viewPlacesDetails = (idPuntoDeVenta,idUsuario,ultimoReporte) => {
            if ($('#infoMap').hasClass('active')) {
                sidebar.close('infoMap');
                $('#infoMap').removeClass('active');
                viewPlacesDetails(idPuntoDeVenta, idUsuario, ultimoReporte);
            } else {
                loadInfoPDV(idPuntoDeVenta,idUsuario,ultimoReporte);
                sidebar.open('infoMap');
            }
        }

        const loadInfoPDV = (idPuntoDeVenta,idUsuario,ultimoReporte) =>
            $.ajax({
                url: "@Url.Action("GetInfoPuntoDeVenta", "Geo")",
                type: "POST",
                dataType: "html",
                data: { idPuntoDeVenta: idPuntoDeVenta, usuarioReporte: idUsuario, ultimoReporte: ultimoReporte},
                beforeSend: () => $('#infoMapDetail').empty(),
                success: data=> $("#infoMapDetail").html(data),
                error: errorThrown => console.log(errorThrown)
            });

        const showGeneralMap = (condition = false) => {
            $.each(markersRealtime, (i,m) => m.setVisible(condition));
            $.each(markersPDV, (i, m) => m.setVisible(condition));
            if (mc) {
                (condition ? mc.addMarkers(markersPDV) : mc.clearMarkers());
            }
        }

        /*-----------------------------------------------CONTROL BOTONES -----------------------------------------------*/
        $('#selectUserMap').on('click', () => {
            showGeneralMap(false);
            showUserTracking(true);
        });

        $('#selectGeneralMap').on('click', () => {
            showGeneralMap(true);
            showUserTracking(false);
        });


        /*--------------------------------------------RUTAS DE USUARIO -------------------------------------------------*/

        const showReportDetailWindow = (marker) => {
            if ($('#userMap').hasClass('active'))
                sidebar.close('userMap');

            if ($('#infoMap').hasClass('active')) {
                sidebar.close('infoMap');
                $('#infoMap').removeClass('active');
                showReportDetailWindow(marker);
            } else {
                loadInfoReport(marker);
                sidebar.open('infoMap');
            }
        }

        const showUserTracking = (condition = false) => {
            $.each(markersUserTracking, (i, m) => m.setVisible(condition));
            $.each(markersUserGeo, (i, m) => m.setVisible(condition));
        }

        const removeUserRoute = () => {
            markersUserTracking.forEach(item => item.setMap(null));
            markersUserTracking.splice(0);

            markersUserGeo.forEach(item => item.setMap(null));
            markersUserGeo.splice(0);

            userTrackingWithData();
        }

        const setUserRoute = (data) => {
            $.each(data, (key, { lat,lng,label}) => {
                const marker = new google.maps.Marker({
                    position: new google.maps.LatLng(lat, lng),
                    map: map,
                    icon: {
                        path: google.maps.SymbolPath.CIRCLE,
                        fillColor: '#66D',
                        fillOpacity: 0.6,
                        strokeColor: '#88F',
                        strokeOpacity: 0.9,
                        strokeWeight: 1,
                        scale: 5
                    }
                });
                const i = new google.maps.InfoWindow({
                    content: `<h3>${label} HS</h3>`,
                    position: new google.maps.LatLng(lat, lng)
                });
                i.close()

                marker.addListener('mouseover', () => i.open(map, marker));
                marker.addListener('mouseout', () => i.close());
                markersUserGeo.push(marker);
            });
        }

        const setUserRouteMarkers = (data) => {
            $.each(data, (key, value) => {
                const marker = new google.maps.Marker({
                    position: value.position,
                    map: map,
                    icon: "@Url.Content("~/images/")" + value.icon,
                    visitado: value.visitado,
                    idReporte: value.idReporte,
                    idPuntoDeVenta: value.idPuntoDeVenta,
                    label: value.label,
                    operacion: value.operacion,
                    fecha: value.fecha,
                    categoria: value.categoria
                });
                google.maps.event.addListener(marker, 'spider_click', () => showReportDetailWindow(marker));
                markersUserTracking.push(marker);
                oms.addMarker(marker);
            });
        }

        const getTrackingUserList = (nDate) =>
            $.ajax({
                url: "@Url.Action("GetListUserTracking", "Geo")",
                type: "POST",
                dataType: "html",
                data: { date: nDate },
                beforeSend: () => $("._trackingLoader").html('<img src="@Url.Content("~/images/ajax-loader.gif")"/>'),
                success: (data) => $("._trackingUserList").html(data),
                complete: () => $("._trackingLoader").empty()
            });

        const getRutaDeUsuario = (idUsuario, newDate) => {

            $.ajax({
                url: "@Url.Action("GetRutaDeUsuario", "Geo")",
                contentType: "application/json; charset=utf-8",
                type: 'POST',
                data: JSON.stringify({
                    idUsuario: idUsuario,
                    FechaHoy: newDate
                }),
                async: false,
                beforeSend: () => removeUserRoute(),
                success: data => {
                    if (data && data.marcadores.length > 0) {
                        setUserRoute(data.ruta);
                        setUserRouteMarkers(data.marcadores);
                        map.setCenter({ lat: data.marcadores[0].position.lat, lng: data.marcadores[0].position.lng });
                        map.setZoom(12);

                    } else {
                        userTrackingWithData();
                        userTrackingNoDataMessage();
                    }
                },
                error: (xhr, status, errorThrown) =>  {
                    console.log('----------------- getMarcadores ------------------');
                    console.log(xhr);
                    console.log(status);
                    console.log(errorThrown);
                    console.log('--------------------------------------------------');
                }
            });
        }

        const loadInfoReport = (marker)  => {
        if (marker.operacion && marker.operacion.length > 0) {
            infoWindow = new google.maps.InfoWindow({
                content: `<h2>${marker.operacion}</h2><p>${marker.fecha}</p>`
            });
            infoWindow.open(map, marker);
        }
        else {
            $.ajax({
                url: '@Url.Action("GetInfoWindowReporte", "Geo")',
                method: 'POST',
                dataType: 'html',
                data: { IdReporte: marker.idReporte },
                beforeSend: () => $('#infoMapDetail').empty(),
                success: data => $("#infoMapDetail").html(data),
                error: err => console.log(err)
            });
        }
        }



        /*------------------------------------AMPLIAR IMAGENES, no sabia bien donde ponerlo -------------------------------------------*/
        const zoomImageItem = (element) => {
            let image = element.innerHTML;
            $('#modalAmpliarImagen').find('.modal-content').html(element.innerHTML);
