@using Reporting.ViewModels;
@model UsuarioRolViewModel
<li>
    @Html.HiddenFor(model=>model.RolId)
    @Html.HiddenFor(model=>model.Name)
    @Html.CheckBoxFor(x => x.Selected, new { @class = "chkRol", @id = Model.RolId })
    @Html.LabelFor(x => x.Selected, Model.Name, new { style = "font-size:8" })
</li>
